CREATE TABLE teams(
	id INTEGER PRIMARY KEY,
  	team_name TEXT,
  	location TEXT,
  	CONFERENCE TEXT
);

CREATE TABLE players(
	id INTEGER PRIMARY KEY,
  	first_name TEXT,
  	last_name TEXT,
  	age INTEGER,
  	position TEXT,
  	team_id TEXT ,
  	FOREIGN KEY(team_id) REFERENCES teams(id)
);

