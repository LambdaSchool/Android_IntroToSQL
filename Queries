SELECT * FROM players;

SELECT id FROM players;

SELECT * FROM players WHERE id=2;

SELECT * FROM players WHERE id>2;

UPDATE players SET first_name = "Cap't" WHERE id = 2;

SELECT * FROM players WHERE id = 2;

DELETE FROM players WHERE id = 3;

SELECT * FROM players;


SELECT
players.first_name, players.last_name,
teams.team_name AS Team_Name
FROM
players
INNER JOIN teams ON teams.team_key = players.player_key;

